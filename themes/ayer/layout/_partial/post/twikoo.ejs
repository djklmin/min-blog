<% if (!index && theme.twikoo && theme.twikoo.enable){ %>
    <script src="https://registry.npmmirror.com/twikoo/1.6.44/files/dist/twikoo.min.js"></script>
    <div id="twikoo" class="twikoo"></div>
    <script>
        twikoo.init({
            envId: "<%- theme.twikoo.envId %>",
            el: '#twikoo',
            lang: 'zh-CN'
        })
    </script>
<% } %>